<script>
  function getRegion(pincode){
    if(pincode.length==6){
      $.ajax({
        url: '/app/regions/cities?pincode='+pincode,
        type:'GET',
        success: function(data) {
          $('#city').html(data);
        }
      });

      $.ajax({
        url: '/app/regions/districts?pincode='+pincode,
        type:'GET',
        success: function(data) {
          $('#district').html(data);
        }
      });

      $.ajax({
        url: '/app/regions/state?pincode='+pincode,
        type:'GET',
        success: function(data) {
          $('#state').val(data);
        }
      })
    }
  }
